{
  "version": 2,
  "builds": [
    { "src": "/api/authentication.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/authentication", "dest": "/api/authentication.js" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "DEV_MONGODB_URI": "@dev_mongodb_uri",
    "PRIVATE_KEY": "@private_key",
    "PUBLIC_KEY": "@public_key"
  }
}
